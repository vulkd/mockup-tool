{
    "name": "byzantine-server",
    "version": "1.0.0",
    "main": "index.js",
    "private": true,
    "engines": {
        "node": ">=10.0.0"
    },
    "scripts": {
        "start": "node index.js",
        "dev": "NODE_ENV=development nodemon index.js --ignore test/",
        "dev-inspect": "NODE_ENV=development nodemon --inspect index.js --ignore test/",
        "dev-test-server": "NODE_ENV=test nodemon index.js --ignore test/",
        "dev-test-server-inspect": "NODE_ENV=test nodemon --inspect index.js --ignore test/ --stack-trace-limit=1000",
        "test-server": "NODE_ENV=test node index.js",
        "lint": "eslint --ext .js ./*.js controllers mailer middleware models routes util",
        "lint-test": "eslint --ext .js ./*.js test",
        "test": "NODE_ENV=test mocha --recursive --timeout 50000 --bail --exit test",
        "test-coverage": "NODE_ENV=test nyc --reporter=text mocha --recursive --timeout 10000 --bail --exit",
        "test-unit": "NODE_ENV=test mocha --recursive --timeout 50000 --bail --exit \"test/unit/*.spec.js\"",
        "test-account": "NODE_ENV=test mocha --recursive --timeout 50000 --bail --exit \"test/api/account/*.spec.js\"",
        "test-public": "NODE_ENV=test mocha --recursive --timeout 50000 --bail --exit \"test/api/public.spec.js\"",
        "docs": "jsdoc -d docs ./server.js ./cronjobs.js util/*.js controllers mailer middleware models routes util"
    },
    "dependencies": {
        "bcrypt-nodejs": "^0.0.3",
        "config": "^3.0.1",
        "cookie-parser": "^1.4.4",
        "cors": "^2.8.5",
        "cron": "^1.7.1",
        "date-fns": "^2.9.0",
        "dotenv": "^6.2.0",
        "email-templates": "^6.1.1",
        "express": "^4.16.4",
        "express-pino-logger": "^4.0.0",
        "file-type": "^12.0.0",
        "helmet": "^3.19.0",
        "ip": "^1.1.5",
        "ip-regex": "^4.1.0",
        "jsonwebtoken": "^8.5.1",
        "knex": "^0.19.5",
        "lodash": "^4.17.15",
        "multer": "^1.4.2",
        "node-telegram-
        "nodemailer": "^6.2.1",
        "nodemailer-stub-transport": "^1.1.0",
        "pug": "^2.0.4",
        "punycode": "^2.1.1",
        "rate-limiter-flexible": "^1.1.0",
        "redis": "^2.8.0",
        "request": "^2.88.0",
        "request-promise-native": "^1.0.7",
        "sharp": "^0.24.0",
        "sort-keys": "^4.0.0",
        "sqlite3": "^4.1.1",
        "stringz": "^2.0.0",
        "stripe": "^7.14.0",
        "tmp": "^0.1.0",
        "uuid": "^3.3.3",
        "validator": "^11.1.0"
    },
    "devDependencies": {
        "chai": "^4.2.0",
        "chai-as-promised": "^7.1.1",
        "chai-http": "^4.3.0",
        "eslint": "^6.0.1",
        "eslint-plugin-jsdoc": "^15.9.2",
        "eslint-plugin-mocha": "^5.3.0",
        "eslint-plugin-no-secrets": "^0.3.3",
        "eslint-plugin-node": "^9.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-security": "^1.4.0",
        "jsdoc": "^3.6.3",
        "mocha": "^6.1.4",
        "node-mocks-http": "^1.8.0",
        "nyc": "^14.1.1",
        "redis-mock": "^0.47.0",
        "sinon": "^7.4.2"
    }
}
